<% content_for :crumb do %>
	<li><%= link_to("All domains", domains_url()) %> </li>
	<li><%= link_to("#{@domain.name.to_s.capitalize} details", domain_url(@domain)) %> </li>
	<li>New service</li>
<% end %>

<h1>New Service</h1>
<fieldset>
 <legend>Upload zip</legend>
<% form_tag domain_services_url(@domain), :multipart => true do -%>
	<label for="file">Upload zipped/tarred service</label> <%= file_field_tag "file" %>
  	<%= submit_tag 'Upload !' %>
<% end -%>
</fieldset>
